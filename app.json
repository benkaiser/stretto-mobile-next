{
  "expo": {
    "name": "stretto-next-mobile",
    "description": "Stretto Mobile app for stretto-next player",
    "slug": "stretto-next-mobile",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
